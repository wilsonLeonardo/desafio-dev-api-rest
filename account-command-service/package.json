{
  "name": "account_command_service",
  "version": "0.0.1",
  "description": "Account Command Service API",
  "main": "src/index.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "babel src --extensions \".ts\" --out-dir dist --copy-files --no-copy-ignored",
    "lint:tsc": "tsc --project tsconfig.json --noEmit",
    "format": "eslint --fix . && prettier --write .",
    "seeds:generate": "ts-node --project tsconfig.json ./development/seeds/generateSeeds.ts",
    "seeds:clearDb": "ts-node --project tsconfig.json ./development/seeds/clearDatabase.ts",
    "test:coverage": "npm run test -- --coverage",
    "test:unit": "jest --bail --forceExit --passWithNoTests \"tests/unit/.*\\.test\\.ts\"",
    "test:file": "jest --bail --forceExit --passWithNoTests",
    "test": "jest --bail --forceExit --testPathIgnorePatterns \"tests/e2e/.*\\.test\\.ts\"",
    "test:integration": "jest --bail --forceExit --passWithNoTests \"tests/integration/.*\\.test\\.ts\"",
    "test:e2e": "jest --verbose --runInBand --passWithNoTests \"tests/e2e/.*\\.test\\.ts\"",
    "lint": "npm run lint:tsc && eslint . && prettier --check \"src/**/*.(ts|tsx)\""
  },
  "author": "Wilson",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.17.12",
    "@babel/node": "^7.17.10",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-decorators": "^7.17.12",
    "@babel/plugin-transform-flow-strip-types": "^7.17.12",
    "@babel/preset-env": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@types/express": "^4.17.13",
    "axios": "^1.7.7",
    "babel-plugin-module-resolver": "^5.0.0",
    "dotenv": "^16.0.1",
    "dynamoose": "^4.0.1",
    "express": "^4.18.1",
    "http-status": "^1.5.2",
    "joi": "^17.5.0",
    "kafkajs": "^2.2.4",
    "nock": "^13.3.0",
    "redis": "^4.1.0",
    "uuid": "^8.3.2",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.1.0",
    "@types/faker": "^5.1.4",
    "@types/jest": "^27.5.1",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.5.1",
    "supertest": "^6.2.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.5.2"
  }
}
